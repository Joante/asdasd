<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.SeccionColectivo" table="seccionColectivo">
		<id column="idSeccion" name="idSeccion" type="long">
			<generator class="identity"/>
		</id>
		<property column="descripcion" name="descripcion" type="string"/>
		<property column="valorSeccionComun" name="valorSeccionComun" type="double"/>
		<property column="valorSeccionTarifaSocial" name="valorSeccionTarifaSocial" type="double"/>

	</class>
</hibernate-mapping>