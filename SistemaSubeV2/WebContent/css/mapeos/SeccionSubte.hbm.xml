<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.SeccionSubte" table="seccionSubte">
		<id column="idSeccionSubte" name="idSeccionSubte" type="long">
			<generator class="identity"/>
		</id>
			<property column="cantidadViajesMinimo" name="cantidadViajesMinimo" type="int"/>
			<property column="cantidadViajesMaximo" name="cantidadViajesMaximo" type="int"/>	
			<property column="valorSubteNormal" name="valorSubteNormal" type="double"/>
			<property column="valorSubteTarifaSocial" name="valorSubteTarifaSocial" type="double"/>

	</class>
</hibernate-mapping>